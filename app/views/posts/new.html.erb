<h2>new post</h2>

<%= form_for @new_post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, autofocus: true %>
</br>

  <%= f.label :content %>
  <%= f.text_area :content, size: "24x6" %>
  <br>
  <%= f.hidden_field :user_id %>
  <%= f.submit %>
<% end %>
